<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="624.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.lawena.views.tf2.TF2Presenter">
   <children>
      <GridPane hgap="2.0" vgap="2.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" percentHeight="32.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TitledPane fx:id="launchPane" animated="false" collapsible="false" maxHeight="1.7976931348623157E308" prefWidth="438.0" text="%ui.tf2.launch.title" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
               <content>
                  <VBox prefWidth="200.0" spacing="5.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" text="%ui.tf2.launch.resolution" HBox.hgrow="NEVER" />
                              <Spinner fx:id="width" editable="true" maxWidth="1.7976931348623157E308" prefWidth="85.0" />
                              <Label text="%ui.tf2.launch.by" HBox.hgrow="NEVER" />
                              <Spinner fx:id="height" editable="true" maxWidth="1.7976931348623157E308" prefWidth="85.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" text="%ui.tf2.launch.dxlevel" HBox.hgrow="NEVER" />
                              <ComboBox fx:id="dxlevel" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="50.0" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" HBox.hgrow="NEVER" />
                              <CheckBox fx:id="insecure" mnemonicParsing="false" prefHeight="25.0" text="%ui.tf2.launch.insecure" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" HBox.hgrow="NEVER" />
                              <CheckBox fx:id="defaultLaunch" mnemonicParsing="false" prefHeight="25.0" text="%ui.tf2.launch.default" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox alignment="BOTTOM_RIGHT" layoutX="20.0" layoutY="80.0" spacing="5.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#showLaunchOptionsDialog" text="%ui.tf2.launch.advanced" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </content></TitledPane>
            <TitledPane fx:id="recorderPane" animated="false" collapsible="false" maxHeight="1.7976931348623157E308" text="%ui.tf2.recorder.title" GridPane.columnIndex="1" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
               <content>
                  <VBox prefWidth="200.0" spacing="5.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <ComboBox fx:id="captureMode" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                              <Label text="%ui.tf2.recorder.at" HBox.hgrow="NEVER" />
                              <Spinner fx:id="fps" editable="true" prefWidth="80.0" HBox.hgrow="NEVER" />
                              <Label text="%ui.tf2.recorder.fps" HBox.hgrow="NEVER" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="5.0">
                           <children>
                              <Label text="%ui.tf2.recorder.to" />
                              <TextField fx:id="framesPath" maxWidth="1.7976931348623157E308" promptText="%ui.tf2.recorder.framesPathPrompt" HBox.hgrow="ALWAYS" />
                              <Button mnemonicParsing="false" onAction="#showFramesPathChooser" text="...">
                                 <tooltip>
                                    <Tooltip text="%ui.tf2.recorder.selectFolderTooltip" />
                                 </tooltip>
                              </Button>
                           </children>
                        </HBox>
                        <VBox fx:id="recorderInfo" VBox.vgrow="ALWAYS" />
                     </children></VBox>
               </content></TitledPane>
            <TitledPane fx:id="configPane" animated="false" collapsible="false" maxHeight="1.7976931348623157E308" text="%ui.tf2.config.title" GridPane.rowIndex="1" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
               <content>
                  <VBox prefWidth="200.0" spacing="5.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" text="%ui.tf2.config.hud" HBox.hgrow="NEVER" />
                              <ComboBox fx:id="hud" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" text="%ui.tf2.config.skybox" HBox.hgrow="NEVER" />
                              <ComboBox fx:id="skybox" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <Label prefWidth="80.0" text="%ui.tf2.config.viewmodels" HBox.hgrow="NEVER" />
                              <ComboBox fx:id="vmSwitch" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                              <Label fx:id="withLabel" text="%ui.tf2.config.with" HBox.hgrow="NEVER" />
                              <Spinner fx:id="vmFov" editable="true" maxWidth="1.7976931348623157E308" prefWidth="80.0" HBox.hgrow="NEVER">
                                 <tooltip>
                                    <Tooltip text="%ui.tf2.config.vmFovTooltip" />
                                 </tooltip>
                              </Spinner>
                              <Label fx:id="vmFovLabel" text="%ui.tf2.config.vmFov" HBox.hgrow="NEVER" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <TableView fx:id="configFlagTable" editable="true" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox alignment="BOTTOM_RIGHT" spacing="5.0">
                           <children>
                              <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#showCustomSettingsDialog" text="%ui.tf2.launch.custom" />
                           </children>
                        </HBox>
                     </children></VBox>
               </content></TitledPane>
            <TitledPane fx:id="resourcesPane" animated="false" collapsible="false" maxHeight="1.7976931348623157E308" text="%ui.tf2.resources.title" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
               <content>
                  <VBox prefWidth="200.0" spacing="5.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                           <children>
                              <Label alignment="TOP_LEFT" text="%ui.tf2.resources.header" wrapText="true" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" spacing="5.0">
                           <children>
                              <TableView fx:id="resourcesTable" editable="true" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                     </children></VBox>
               </content></TitledPane>
         </children>
         <padding>
            <Insets left="3.0" right="3.0" top="3.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
