<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="mainContainer" prefHeight="640.0" prefWidth="750.0" stylesheets="@base.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.lawena.views.base.BasePresenter">
    <children>
        <TabPane fx:id="tabs" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab fx:id="setupTab" closable="false" text="%ui.base.tabs.setup">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <ProgressIndicator fx:id="placeholder" maxHeight="50.0" maxWidth="50.0" style="-fx-accent: #5319e7;" BorderPane.alignment="CENTER" />
                     </center>
                  </BorderPane>
               </content></Tab>
                <Tab fx:id="renderingTab" closable="false" text="%ui.base.tabs.rendering" />
                <Tab fx:id="demosTab" closable="false" text="%ui.base.tabs.demos" />
                <Tab fx:id="tasksTab" closable="false" text="%ui.base.tabs.tasks">
                    <content>
                        <VBox fx:id="tasksPane" spacing="5.0">
                            <padding>
                                <Insets left="3.0" right="3.0" top="3.0" />
                            </padding>
                        </VBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <HBox id="HBox" alignment="BOTTOM_LEFT" spacing="5.0" VBox.vgrow="SOMETIMES">
            <children>
                <ComboBox fx:id="profilesComboBox" prefWidth="150.0" />
                <SplitMenuButton fx:id="profileSplitMenuButton" mnemonicParsing="false" onAction="#duplicateProfile" text="%ui.base.buttons.quickNewProfile">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#newProfile" text="%ui.base.buttons.newProfile" />
                        <MenuItem mnemonicParsing="false" onAction="#renameProfile" text="%ui.base.buttons.renameProfile" />
                        <MenuItem mnemonicParsing="false" onAction="#deleteProfile" text="%ui.base.buttons.deleteProfile" />
                    </items>
                </SplitMenuButton>
                <Label fx:id="leftStatus" maxWidth="-1.0" prefHeight="25.0" HBox.hgrow="ALWAYS" />
                <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                <Label fx:id="rightStatus" contentDisplay="RIGHT" maxWidth="-1.0" prefHeight="25.0" HBox.hgrow="NEVER">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Label>
                <Button fx:id="launchButton" mnemonicParsing="false" onAction="#launch" prefWidth="200.0" text="%ui.base.buttons.launchGame" />
            </children>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </padding>
        </HBox>
    </children>
</VBox>
