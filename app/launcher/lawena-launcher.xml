<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
    <dontWrapJar>true</dontWrapJar>
    <headerType>gui</headerType>
    <jar>.\lawena\getdown-client.jar</jar>
    <outfile>lawena.exe</outfile>
    <errTitle></errTitle>
    <cmdLine>lawena</cmdLine>
    <chdir>.</chdir>
    <priority>normal</priority>
    <downloadUrl>http://www.oracle.com/technetwork/java/javase/downloads/index.html</downloadUrl>
    <supportUrl>https://github.com/iabarca/lawena-recording-tool/issues</supportUrl>
    <stayAlive>false</stayAlive>
    <manifest>elevated.manifest</manifest>
    <icon>cap_icon_64.ico</icon>
    <jre>
        <path></path>
        <bundledJre64Bit>false</bundledJre64Bit>
        <minVersion>1.7.0_41</minVersion>
        <maxVersion></maxVersion>
        <jdkPreference>preferJre</jdkPreference>
        <runtimeBits>64/32</runtimeBits>
    </jre>
    <splash>
        <file>splash.bmp</file>
        <waitForWindow>true</waitForWindow>
        <timeout>60</timeout>
        <timeoutErr>true</timeoutErr>
    </splash>
    <versionInfo>
        <fileVersion>4.2.2.0</fileVersion>
        <txtFileVersion>4.2.2</txtFileVersion>
        <fileDescription>Launcher and Updater for Lawena Recording Tool</fileDescription>
        <copyright>Lawena</copyright>
        <productVersion>4.2.0.0</productVersion>
        <txtProductVersion>4.2.0</txtProductVersion>
        <productName>Lawena v4 Launcher and Updater</productName>
        <companyName>Lawena</companyName>
        <internalName>lawena</internalName>
        <originalFilename>lawena.exe</originalFilename>
    </versionInfo>
</launch4jConfig>